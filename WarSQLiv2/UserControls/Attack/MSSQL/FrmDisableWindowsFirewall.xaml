        <RichTextBox x:Name="txtStatus" HorizontalAlignment="Left" Height="143" Margin="10,102,0,0" VerticalAlignment="Top" Width="451" Cursor="" AllowDrop="True" VerticalScrollBarVisibility="Visible" IsReadOnly="True">
