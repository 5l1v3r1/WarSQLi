            </FlowDocument>
        </RichTextBox>
        <Button x:Name="btnEnable" Content="Enable" HorizontalAlignment="Left" Margin="216,31,0,0" VerticalAlignment="Top" Width="114" Click="btnEnable_Click"/>
        <Button x:Name="btnDisable" Content="Disable" HorizontalAlignment="Left" Margin="343,31,0,0" VerticalAlignment="Top" Width="114" Click="btnDisable_Click"/>

    </Grid>
</Window>
